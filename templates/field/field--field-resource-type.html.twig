{#
/**
 * @file
 * Theme override for 'field_resource_type' on 'node' entities.
 * Displays the resource type as a badge without the default label/wrapper.
 */
#}
<div class="mb-6">
  {% for item in items %}
  <span class="inline-flex items-center px-2.5 py-0.5 rounded-full text-xs font-medium bg-secondary text-secondary-foreground">
    {{ item.content }}
  </span>
  {% endfor %}
</div>