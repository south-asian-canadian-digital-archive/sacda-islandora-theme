{#
/**
 * @file
 * Theme override to display a node of type 'collection'.
 * Simple layout with title, description, and children content.
 */
#}

<article{{ attributes.addClass('node-collection') }}>



		{% if content.body|render|striptags|trim %}
			<div class="prose max-w-none text-foreground leading-relaxed mb-8">
				{{ content.body }}
			</div>
		{% endif %}

		{{ content|without('body') }}

		<div class="mt-8 border-t border-border pt-8">
			<h3>Fonds/Collection Contents</h3>
			

		</div>
</article>
